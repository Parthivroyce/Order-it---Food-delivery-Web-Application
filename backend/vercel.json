{
    "version": 2,
    "routes": [
      {
        "src": "/api/food/(.*)",
        "dest": "/api/foodRoute.js"
      },
      {
        "src": "/images/(.*)",
        "dest": "/uploads/$1"
      },
      {
        "src": "/api/user/(.*)",
        "dest": "/api/userRoute.js"
      },
      {
        "src": "/api/cart/(.*)",
        "dest": "/api/cartRoute.js"
      },
      {
        "src": "/api/order/(.*)",
        "dest": "/api/orderRoute.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ]
  }
  