{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/food(.*)",
      "dest": "server.js"
    },
    {
      "src": "/images/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/api/user(.*)",
      "dest": "server.js"
    },
    {
      "src": "/api/cart(.*)",
      "dest": "server.js"
    },
    {
      "src": "/api/order(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
